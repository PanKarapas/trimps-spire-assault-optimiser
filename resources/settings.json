{
    "runner": {
        "trimpsIndexFilepath": "./src/trimps/index.html",
        "maxParallelPages": 8,
        "browserLogs": {
            "includeInProgressBars": true,
            "fileToWriteTo": "./resources/browserLogs.log"
        },
        "saveBackupBeforeSimulation": {
            "enabled": true,
            "filepath": "./resources/save.backup.json"
        },
        "levelStart": 1,
        "levelEnd": -1,
        "trimpsSaveFilepath": "./resources/trimpsSave.txt",
        "saveFilePath": "./resources/save.json",
        "headless": true,
        "devtools": false
    },
    "simulator": {
        "name": "Random Combo Simulator",
        "simulatorSettings": {
            "framesPerSimulation": 12000,
            "numberOfRandomSimulations": 1000,
            "updateSaveFile": true
        }
    }
}